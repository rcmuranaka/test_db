        "import tensorflow as tf\n",
        "from tensorflow import keras\n",
        "import numpy as np\n",
        "\n",
        "# --- 1. Carregar e Preparar os Dados ---\n",
        "# Usamos o dataset Fashion MNIST, um conjunto de 70.000 imagens de roupas (28x28 pixels)\n",
        "(imagens_treino, rotulos_treino), (imagens_teste, rotulos_teste) = keras.datasets.fashion_mnist.load_data()\n",
        "\n",
        "# Normalização: Reduz os valores dos pixels de 0-255 para 0-1\n",
        "imagens_treino = imagens_treino / 255.0\n",
        "imagens_teste = imagens_teste / 255.0\n",
        "\n",
        "# Os rótulos (categorias)\n",
        "nomes_classes = ['T-shirt/top', 'Trouser', 'Pullover', 'Dress', 'Coat',\n",
        "               'Sandal', 'Shirt', 'Sneaker', 'Bag', 'Ankle boot']\n",
        "\n",
        "# --- 2. Construir o Modelo (A Rede Neural) ---\n",
        "modelo = keras.Sequential([\n",
        "    # Camada 1: Achata a imagem 28x28 em um array 784 (entrada)\n",
        "    keras.layers.Flatten(input_shape=(28, 28)),\n",
        "    # Camada 2: Uma camada densa (fully connected) com 128 neurônios\n",
        "    keras.layers.Dense(128, activation='relu'),\n",
        "    # Camada 3: Camada de saída com 10 neurônios (um para cada classe)\n",
        "    keras.layers.Dense(10, activation='softmax')\n",
        "])\n",
        "\n",
        "\n",
        "# --- 3. Compilar e Treinar o Modelo ---\n",
        "modelo.compile(optimizer='adam',\n",
        "              loss='sparse_categorical_crossentropy',\n",
        "              metrics=['accuracy'])\n",
        "\n",
        "# Treinamento: O modelo aprende com os dados de treino\n",
        "modelo.fit(imagens_treino, rotulos_treino, epochs=10) # 10 épocas (repetições)\n",
        "\n",
        "# --- 4. Salvar o Modelo ---\n",
        "# O modelo treinado é salvo em um arquivo, que será usado pela aplicação\n",
        "modelo.save('classificador_roupas.h5')\n",
        "print(\"Modelo Salvo!\")"
      ]
